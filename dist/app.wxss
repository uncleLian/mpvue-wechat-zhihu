.article-list .list-date {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 28rpx;
  letter-spacing: 2rpx;
  width: 40%;
  height: 56rpx;
  margin-top: 30rpx;
  border-radius: 100rpx;
  background-color: #0084ff;
}
.article-list .list-item {
  box-sizing: border-box;
  position: relative;
  padding: 30rpx 0;
  margin: 0 32rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.article-list .list-item .item-container .item-title {
  color: #000;
  font-size: 34rpx;
  -webkit-line-clamp: 3;
  text-overflow: ellipsis;
  overflow: hidden;
}
.article-list .list-item .item-container.images {
  display: flex;
  align-items: center;
}
.article-list .list-item .item-container.images .item-title {
  width: 67%;
  padding-right: 30rpx;
}
.article-list .list-item .item-container.images .item-images {
  width: 33%;
  height: 156rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.article-list .list-item .item-container.images .item-images ._img {
  display: block;
  width: 100%;
}
.article-list .list-item .item-container.text {
  width: 100%;
}
.comment-list {
  position: relative;
  width: 100%;
}
.comment-list .list-item {
  box-sizing: border-box;
  position: relative;
  color: #353535;
  font-size: 30rpx;
  padding: 30rpx 0;
  margin: 0 40rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.comment-list .list-item .item-header {
  display: flex;
  align-items: center;
  margin-bottom: 16rpx;
}
.comment-list .list-item .item-header .item-user {
  display: flex;
  align-items: center;
}
.comment-list .list-item .item-header .item-user .item-avatar {
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
  border-radius: 100%;
  overflow: hidden;
}
.comment-list .list-item .item-header .item-user .item-avatar ._img {
  display: block;
  width: 100%;
  height: 100%;
}
.comment-list .list-item .item-header .item-time {
  color: #8590a6;
  font-size: 28rpx;
  margin-left: auto;
}
.comment-list .list-item .item-content {
  margin-bottom: 16rpx;
}
.comment-list .list-item .item-footer {
  display: flex;
  align-items: center;
}
.comment-list .list-item .item-footer .footer-item {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #8590a6;
  font-size: 28rpx;
  line-height: 1;
}
.comment-list .list-item .item-footer .footer-item .item-icon {
  margin-right: 10rpx;
}
.theme-list .list-item {
  box-sizing: border-box;
  position: relative;
  margin: 30rpx 32rpx;
  border: 2rpx solid #ebeef5;
  border-radius: 10rpx;
  box-shadow: 0 4rpx 24rpx 0 rgba(0,0,0,0.1);
  overflow: hidden;
}
.theme-list .list-item .item-container {
  position: relative;
  width: 100%;
  height: 320rpx;
  background-color: #ddd;
  border-radius: 10rpx 10rpx 0 0;
  overflow: hidden;
}
.theme-list .list-item .item-container .item-image {
  display: block;
  width: 100%;
}
.theme-list .list-item .item-container .item-image ._img {
  display: block;
  width: 100%;
}
.theme-list .list-item .item-text {
  padding: 16rpx;
}
.theme-list .list-item .item-text .item-title {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #000;
  font-size: 28rpx;
}
.theme-list .list-item .item-text .item-subTitle {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #353535;
  font-size: 26rpx;
}
.pageLoading {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 200;
  color: #888;
  font-size: 36rpx;
  background: #fff;
  display: flex;
  justify-content: center;
}
.pageLoading .pageLoading-type {
  box-sizing: border-box;
  width: 100%;
  text-align: center;
  padding: 0 32rpx;
  margin-top: 30%;
}
.pageLoading .pageLoading-type .type-label {
  margin-top: 40rpx;
}
.pageLoading .pageLoading-type .type-button {
  margin-top: 40rpx;
  background-color: #0084ff;
}
.pageLoading .pageLoading-type.error .app-button-hover {
  background-color: rgba(0,132,255,0.2);
  opacity: 0.7;
}